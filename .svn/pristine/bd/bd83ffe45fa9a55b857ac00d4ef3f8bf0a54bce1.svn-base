@charset "UTF-8";

label, p {
	font-size:12px;
}

.userInfo {
	width:100%;
	table-layout: fixed;

} 
td {
	padding:1% !important;
}

.col-lg-5{
	margin-left:3%;
	margin-right:3%;
}

.help-block{
	color:#a94442 !important;
}

.header {
	padding-top:15px;
	padding-bottom:15px;
}

.well {
	background-image: #ECECEC;
	border: 0.5px solid #bcbcbc;
}

.icon {
	color:#30BBBB;
}

.center {
	float: none; 
	margin: 0 auto;
}

#privateModal, #servInfModal {
	margin-left:15px;
}

textarea, .btnUpload{
	width:100%;
	resize: none;
}

.user__header{
	margin-bottom:10px;
	border-bottom:1px solid black;
}

.pull-right{
	margin-right:10px;	
}

.file-input{
    position: relative;
    overflow: hidden;
  	margin: 0px;
    color: #333;
    cursor: pointer;
    background-color: #fff;
    border-color: #ccc;
}
.file-input input[type=file] {
	position: absolute;
	top: 0;
	right: 0;
	margin: 0;
	padding: 0;
	cursor: pointer;
	font-size: 20px;
	opacity: 0;
	filter: alpha(opacity=0);
}
.file-input-title {
    margin-left:2px;
}
.files{
	margin-bottom:10px;
}

table.borderless>tbody>tr>td, table.borderless>tbody>tr>th {
     border:none;
}

.pointer{
	cursor:pointer;
}

.dateInputParent {
	display:flex; 
	align-items:center;
}

.dateInput {
	width:48%;
	padding:0;
}

#paging {
	text-align:center;
}

#buttonForWebSubmit,
#buttonForFileSubmit{
	margin-bottom:20px;
}

.searchText{
	padding-top:7px;
}