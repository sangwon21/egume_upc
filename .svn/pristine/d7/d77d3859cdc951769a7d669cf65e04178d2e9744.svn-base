.file-input{
    position: relative;
    overflow: hidden;
  	margin: 0px;
    color: #333;
    cursor: pointer;
    background-color: #fff;
    border-color: #ccc;
}
.file-input input[type=file] {
	position: absolute;
	top: 0;
	right: 0;
	margin: 0;
	padding: 0;
	cursor: pointer;
	font-size: 20px;
	opacity: 0;
	filter: alpha(opacity=0);
}
.file-input-title {
    margin-left:2px;
}
